{
  "name": "fielder-docs",
  "version": "1.0.0",
  "description": "Fielder Docs Site",
  "main": "dist/index.html",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack",
    "build:meta": "react-snap",
    "build:sitemap": "sitemap-static --prefix=https://fielder.andyrichardson.dev/ --pretty --ignore-file=sitemap-ignore.json dist > dist/sitemap.xml",
    "postbuild": "npm run build:meta && npm run build:sitemap",
    "deploy": "(echo \"fielder.andyrichardson.dev\" > dist/CNAME) && gh-pages -d dist"
  },
  "author": "Andy Richardson",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@linaria/webpack-loader": "^4.1.3",
    "@linaria/webpack5-loader": "^4.1.3",
    "@mdx-js/loader": "^2.1.3",
    "@mdx-js/mdx": "^2.1.3",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "favicons": "^6.0.0",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "react-snap": "^1.23.0",
    "remark-prism": "^1.3.6",
    "remark-slug": "^7.0.1",
    "sitemap-static": "^0.4.4",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.12",
    "@fontsource/source-code-pro": "^4.5.12",
    "@linaria/babel-preset": "^4.2.0",
    "@linaria/core": "^4.1.2",
    "@linaria/react": "^4.1.3",
    "@linaria/shaker": "^4.2.0",
    "@mdx-js/react": "^2.1.3",
    "@philpl/buble": "^0.19.7",
    "fielder": "^2.1.1",
    "hoofd": "^1.5.2",
    "polished": "^4.2.2",
    "preact": "^10.11.0",
    "prism": "^4.1.2",
    "prism-themes": "^1.9.0",
    "react-helmet": "^6.1.0",
    "react-live": "^2.4.1",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "wouter": "^2.7.5",
    "wouter-preact": "^2.7.5"
  },
  "reactSnap": {
    "source": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  }
}
